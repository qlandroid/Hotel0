<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#ececec</item>
        <item name="colorPrimaryDark">#ececec</item>
        <item name="colorAccent">#ececec</item>
    </style>

    <style name="div_x">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/x1</item>
        <item name="android:background">@color/divColor</item>
    </style>
    <style name="div_y">
        <item name="android:layout_width">@dimen/x1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/divColor</item>
    </style>

    <style name="detailsNameLayout">
        <item name="android:paddingTop">@dimen/x6</item>
        <item name="android:paddingBottom">@dimen/x6</item>
        <item name="android:paddingLeft">@dimen/x10</item>
        <item name="android:paddingRight">@dimen/x10</item>
    </style>

</resources>
